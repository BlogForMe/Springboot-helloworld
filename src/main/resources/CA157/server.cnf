[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
C  = US
ST = State
L  = City
O  = Organization
OU = OrgUnit
CN = 192.168.31.157  # Server IP

[ req_ext ]
subjectAltName = IP:192.168.31.157  # Must match the server IP
